<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_height="match_parent">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="Sim State"
            android:layout_margin="20dp"
            android:layout_marginTop="40dp"
            android:textSize="20sp"

            app:layout_constraintEnd_toStartOf="@id/text1"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:id="@+id/text1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_margin="20dp"
            android:padding="4dp"
            android:textSize="20sp"
            android:text="Text B"
            android:layout_toRightOf="@id/textA"/>
        <TextView
            android:id="@+id/textC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="ICCID"
            android:layout_below="@id/textA"
            android:layout_margin="20dp"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@id/text2"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:id="@+id/text2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_margin="20dp"
            android:padding="4dp"
            android:layout_below="@id/text1"
            android:textSize="20sp"
            android:text="Text D"
            android:layout_toRightOf="@id/textA"/>

        <TextView
            android:id="@+id/textE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textC"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="ISMI"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@id/text2"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text3"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_margin="20dp"
            android:padding="4dp"
            android:layout_below="@id/textC"
            android:textSize="20sp"
            android:text="Text F"
            android:layout_toRightOf="@id/textA"/>

        <TextView
            android:id="@+id/textG"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textE"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="ISO"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@id/text4"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/text3"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@id/textA"
            android:padding="4dp"
            android:text="Text H"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/textI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textG"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="Serial No"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@id/text5"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/text4"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@id/textA"
            android:padding="4dp"
            android:text="Applicable for API 26 or below"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textJ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textI"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="SDK Version"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@id/text6"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/text5"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@id/textJ"
            android:padding="4dp"
            android:text="Text J"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textJ"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:ellipsize="end"
            android:lines="1"
            android:padding="4dp"
            android:text="OS level"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@id/text7"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/text6"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@id/textK"
            android:padding="4dp"
            android:text="Text K"
            android:textSize="20sp" />


    </RelativeLayout>



